{% extends 'base.html.twig' %}


{% block body %}
    {{ parent() }}

    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}

    {{ form_start(loginForm) }}
    {{ form_widget(loginForm) }}
    {{ form_end(loginForm) }}

    <a href="{{ path('registration') }}">Registration</a>
{% endblock %}